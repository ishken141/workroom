<div class="text-center">
<h2 class="devise">LOGIN</h2>

<%= form_for(resource, as: resource_name, url: session_path(resource_name)) do |f| %>
  <div class="form-group">
    <%= f.label :EMAIL, class: "control"%><br />
    <%= f.email_field :email, autofocus: true, autocomplete: "email" %>
  </div>

  <div class="form-group">
    <%= f.label :PASSWORD, class: "control" %><br />
    <%= f.password_field :password, autocomplete: "current-password" %>
  </div>

  <% if devise_mapping.rememberable? %>
    <div class="form-group">
      <%= f.check_box :remember_me, class: "control" %>
      <%= f.label :remember_me, class: "control" %>
    </div>
  <% end %>

  <div class="actions">
    <%= f.submit "LOGIN", class: "btn btn-primary", id: "bbb" %>
  </div>
<% end %>
</div>
